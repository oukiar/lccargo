</br><table class="pure-table pure-table-bordered" style="position:absolute; top:89px; left:377px; max-width: 70%; min-width: 70%; z-index:8;">    <thead>        <tr>			<th><font style="color: white;">Status</font></th>            <th><font style="color: white;">Arrival</font></th>            <th><font style="color: white;">Departure</font></th>            <th><font style="color: white;">Airline</font></th>			<th><font style="color: white;">Contact Phone</font></th>			<th><font style="color: white;">Destination Address</font></th>		    </tr>    </thead>    <tbody><% reservations.forEach(function(reservation){ %>        <tr id="onhoverTable">			<td><div style='text-align:center;'><img src='<%= reservation.get("Icon") %>' width='25' height='25' alt='Waiting'></div> <%= reservation.get("Status") %></td>			<td><%= reservation.get("Arrival") %></td>			<td><%= reservation.get("Departure") %></td>			<td><%= reservation.get("Airline") %></td>			<td><%= reservation.get("Telephone") %></td>			<td><%= reservation.get("Destination") %></td>			<td><a href="/trip/<%= reservation.id %>"><img src='/web/img/icons/view.png' width="25" height="25" alt="Status" style="cursor:pointer;"></a></td>									<td><img src='<%= reservation.get("Status2")%>' id='<%= reservation.get("Status") %>' width="25" height="25" alt="Status" onclick="<%= reservation.get("Status")%>('<%= reservation.id %>', this.id)" style="cursor:pointer;"></td>        </tr> <% }) %>    </tbody></table>