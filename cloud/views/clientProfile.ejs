<!doctype html><html lang="en"><head>     <meta charset="utf-8"> <meta name="viewport" content="width=device-width, initial-scale=1.0"> <meta name="description" content="LC Cargo Xpress, cargo company, export, fashion district"> <title><%= user.get("Name") %></title> <link rel="stylesheet" href="cod/css/pure2-min.css"> <link rel="stylesheet" href="cod/css/grids-responsive-min.css">     <link rel="stylesheet" href="cod/css/style.css"></head><body>	<% var status = user.get("Status") %>	<% var usrClass = Parse.User.current().get("Class"); %>	<% if( status === "Pending" && usrClass === "staff") {%>			<% include partitions/pmenu.ejs %><% include partitions/navmenu.ejs %></br>		<table align="center" class="NoHover">			<tr>				<td><a href="/mainclients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Active Clients</b></font></a></td>				<td><a href="/clients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Pending</b></font></a></td>					<td><a href="/reservations" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Reservations</b></font></a></td>			</tr>		</table>	</br><div id="mainDiv"><fieldset id="mainField"><table class="NoHover"><tr>	<td><img src="/web/img/icons/client.png" width="100" height="100" alt="Pending"></td><td><p class="BigOrange">Pending</p></td> </tr></table></br></br><table class="NoHover">	<tr>		<td>   <form name="addClientUser" class="pure-form" action="/addClient" method="post">	  <label>Username:</label>	  <input type="text" name="clientUsrname" value="<%= user.get("Email") %>" size="34">	  <label>Create a Password:</label>	   <input type="password" name="clientpwd" size="21">	   <!--hidden values to store in the clients user-->	   <input type="hidden" name="clientEmail" value="<%= user.get("Email") %>">	   <input type="hidden" name="clientID" value="<%= user.id %>">	    <button type="submit" class="pure-button pure-button-primary" id="submitClient" style="length:50px;">Activate</button>	 </form> </td></tr></table>		<h1 class="para-3"><span class="font-3">Client's Information:</span></h1>	<% include partitions/clientsInfo.ejs %>		</fieldset>	</div>		<%} else if( status == "Active" && usrClass=="staff") {%> 		<% include partitions/pmenu.ejs %>				<% include partitions/navmenu.ejs %>		</br>				<table align="center" class="NoHover">					<tr>						<td><a href="/mainclients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Active Clients</b></font></a></td>						<td><a href="/clients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Pending</b></font></a></td>							<td><a href="/reservations" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Reservations</b></font></a></td>					</tr>				</table>			</br>		<div id="mainDiv">		<fieldset id="mainField">		</br>		<span class="font-3">Client's Information-<%= user.get("Name")%></span>		<table align="right" class="NoHover">					<tr>				<td><a href="#" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Client's Information</b></font></a></td>						<td><a href="#" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>WareHouse</b></font></a></td>						<td><a href="/cl_reserv/<%=user.id%>" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Reservations</b></font></a></td>					</tr>				</table></br></br></br>	<% include partitions/clientsInfo.ejs %></fieldset></div>		<% } %></body></html>