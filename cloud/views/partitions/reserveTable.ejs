</br><table class="pure-table pure-table-bordered" >    <thead>        <tr>			<th><font style="color: white;">Status</font></th>            <th><font style="color: white;">Arrival</font></th>            <th><font style="color: white;">Departure</font></th>            <th><font style="color: white;">Airline</font></th>			<th><font style="color: white;">Email</font></th>			<th><font style="color: white;">Phone</font></th>			<th><font style="color: white;">Destination</font></th>		    </tr>    </thead>    <tbody><% reservations.forEach(function(reservation){ %>        <tr id="onhoverTable">			<td><div style='text-align:center;'><img src='<%= reservation.get("Icon") %>' width='25' height='25' alt='Status' style="cursor:pointer;"></div><p class="pfont"> <%= reservation.get("Status") %></p></td>			<td><p class="pfont"><%= reservation.get("Arrival") %></p></td>			<td><p class="pfont"><%= reservation.get("Departure") %></p></td>			<td><p class="pfont"><%= reservation.get("Airline") %></p></td>			<td><p class="pfont"><%= reservation.get("Email") %></p></td>			<td><p class="pfont"><%= reservation.get("Telephone") %></p></td>			<td><p class="pfont"><%= reservation.get("Destination") %></p></td>			<td><a href="/trip/<%= reservation.id %>"><img src='/web/img/icons/view.png' width="25" height="25" alt="Status" style="cursor:pointer;"></a></td>									<td><img src='<%= reservation.get("Status2")%>' id='<%= reservation.get("Status") %>' width="25" height="25" alt="Status" onclick="<%= reservation.get("Status")%>('<%= reservation.id %>', this.id)" style="cursor:pointer;"></td>        </tr> <% }) %>    </tbody></table>