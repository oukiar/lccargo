<table class="pure-table pure-table-bordered" align="center" style="width:100%;  border: 2px solid #B8C8D2;"> 	<tr> 	<th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Status:</th> 	<% if(user.get("Status")=== "Pending"){ %>     <td><p class="smallOrange"><%= user.get("Status") %></p></td> 	<%}else if(user.get("Status")==="Active"){%>    <td><p class="smallGreen" ><%= user.get("Status") %></p></td>    <% } %> 	</tr>  <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Name:</th>    <td><p class="pfont"><%= user.get("Name") %></p></td>  </tr>  <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Company:</th>    <td><p class="pfont"><%= user.get("Company") %></p></td>  </tr>  <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Country:</th>    <td><p class="pfont"><%= user.get("Country") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">City:</th>    <td><p class="pfont"><%= user.get("City") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Destination:</th>    <td><p class="pfont"><%= user.get("Destination") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Address:</th>    <td><p class="pfont"><%= user.get("Address") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Address 2:</th>    <td><p class="pfont"><%= user.get("Address2") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">ID1:</th>    <td><p class="pfont"><%= user.get("ID1") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">ID2:</th>    <td><p class="pfont"><%= user.get("ID2") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Telephone:</th>    <td><p class="pfont"><%= user.get("Telephone") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Email:</th>    <td><p class="pfont"><%= user.get("Email") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Client ID:</th>    <td><p class="pfont"><%= user.id %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Notes:</th>    <td><p class="pfont"><%= user.get("Notes") %></p></td>  </tr>    <tr>    <th style="width:21%; background-color: #748794;"><a style="color:white" class="pfont">Last Update:</th>    <td><p class="pfont"><%= user.updatedAt %></p></td>  </tr> </table>