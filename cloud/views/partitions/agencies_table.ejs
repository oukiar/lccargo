<table class="pure-table pure-table-bordered" align="left" >    <thead>        <tr>			            <th><font style="color: white;">Name</font></th>            <th><font style="color: white;">Website</font></th>            <th><font style="color: white;">Address</font></th>			<th><font style="color: white;">Country</font></th>			<th><font style="color: white;">State</font></th>			<th><font style="color: white;">Telephone</font></th>			<th><font style="color: white;">Email</font></th>			<th><font style="color: white;">Status</font></th>		    </tr>    </thead>    <tbody>		<% agencies.forEach(function(agency) { %>			        <tr id="onhoverTable">            <td><a href='/agency/<%=agency.id%>' style="text-decoration: none;"><p class="pfont"><%= agency.get("Name") %></p></a></td>            <td><a href="http://<%=agency.get('Website')%>" target="blank" style="text-decoration: none;"><p class="pfont"><%= agency.get("Website") %></p></a></td>			<td><a href="http://maps.google.com/?q=<%=agency.get('Address')%><%=agency.get('ZIP')%> <%=agency.get('State')%>" target="blank" style="text-decoration: none;"><p class="pfont"><%= agency.get("Address") %></p></a></td>			<td><p class="pfont"><%= agency.get("Country") %></p></td>			<td><p class="pfont"><%= agency.get("State") %></p></td>			<td><p class="pfont"><%= agency.get("Telephone") %></p></td>			<td><a href='mailto:<%=agency.get("Email")%>' style="text-decoration: none;"><p class="pfont"><%= agency.get("Email") %></p></a></td>			<td><a href='/agency/<%= agency.id %>' style="cursor:pointer text-decoration: none;"><p class="pfont"><%= agency.get("Status") %></p></a></td>        </tr> <% }) %>     </tbody></table>