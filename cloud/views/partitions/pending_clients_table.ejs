</br><table class="pure-table pure-table-bordered" align="center" style="max-width: 1070px; min-width: 200px;">    <thead>        <tr>            <th><font style="color: white;">Name</font></th>            <th><font style="color: white;">Company</font></th>            <th><font style="color: white;">Country</font></th>			<th><font style="color: white;">Address</font></th>			<th><font style="color: white;">Tel</font></th>			<th><font style="color: white;">Email</font></th>			<th><font style="color: white;">Status</font></th>		    </tr>    </thead>    <tbody>		<% clients.forEach(function(client) { %>			        <tr id="onhoverTable">            <td><a href='/<%=client.id%>' style="text-decoration: none;" target="blank"><%= client.get("Name") %></a></td>            <td><%= client.get("Company") %></td>			<td><%= client.get("Country") %></td>			<td><%= client.get("Address") %></td>			<td><%= client.get("Telephone") %></td>			<td><%= client.get("Email") %></td>			<td><a href='/<%= client.id %>' style="cursor:pointer text-decoration: none;"><%= client.get("Status") %></a></td>        </tr> <% }) %>     </tbody></table>