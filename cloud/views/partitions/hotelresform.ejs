<form name="tripReservation"  action="/reserveHotel" method="post"><div class="section"> <div class="cont">	<div class="colwrapper">	  <div class="cont-2">		<div class="cont-3">		  <div class="cont-4"><p class="para"><span class="font">Reserve your Arrival:</span></p></div>			</div>			  <div class="cont-5">				<div class="cont-6"><p class="para-2"><span class="font-2">Arrival Date:</span></p></div>				  </div>					<div class="cont-7">					  <input type="text" name="arrival_date" class="input">						</div>						<div class="cont-8">							<div class="cont-9"><p class="para-2"><span class="font-2">Departure Date:</span></p></div>						</div>						<div class="cont-10">							<input type="text" name="departure_date" class="input-2">						</div>						<div class="cont-11">							<div class="cont-12"><p class="para-4"><span class="font-4">Airline:</span></p></div>						</div>						<div class="cont-13">							<input type="text" name="airline" class="input-3">						</div>						<div class="cont-14">							<div class="cont-15"><p class="para-5"><span class="font-5">Flight Number:</span></p></div>						</div>						<div class="cont-16">							<input type="text" name="flight_number" class="input-4">						</div>						<div class="cont-17">							<div class="cont-18"><p class="para-6"><span class="font-6">Number of People:</span></p></div>						</div>						<div class="cont-19">							<input type="text" name="number_people" class="input-5">						</div>						<div class="cont-20">							<div class="cont-21"><p class="para-7"><span class="font-7">Contact Telephone:</span></p></div>						</div>						<div class="cont-22">							<input type="text" name="contact_tel" class="input-6" value="<%= user.get("Telephone") %>">						</div>						<div class="cont-23">							<div class="cont-24"><p class="para-8"><span class="font-8">Destination Address:</span></p></div>						</div>						<div class="cont-25">							<input type="text" name="destination" class="input-7">						</div>						<div class="cont-26">							<input type="text" name="destination2" class="input-8">						</div>						<div class="cont-27">							<div class="cont-28"><p class="para-9"><span class="font-9">Email Address:</span></p></div>						</div>						<div class="cont-29">							<input type="text" name="contact_email" class="input-9" value="<%= user.get("Email") %>">						</div>						<div class="cont-30">							<div class="cont-31"><p class="para-10"><span class="font-10">Notes:</span></p></div>						</div>						<div class="cont-32">							<input type="text" name="notes" class="input-10">							<input type="hidden" name="clientID" value="<%= user.id %>">							<input type="hidden" name="clientName" value="<%= user.get("Name") %>">						</div>						<div class="cont-33">							<button type="submit"class="button" href="">Reserve</button>						</div>					</div>				</div>			</div>		</div>		</form>