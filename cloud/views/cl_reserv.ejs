<!doctype html><html lang="en"><head>     <meta charset="utf-8"> <meta name="viewport" content="width=device-width, initial-scale=1.0"> <meta name="description" content="LC Cargo Xpress, cargo company, export, fashion district"> <title><%= user.get("Name") %></title> <link rel="stylesheet" href="../cod/css/pure-min.css"> <link rel="stylesheet" href="../cod/css/grids-responsive-min.css">     <link rel="stylesheet" href="../cod/css/style.css">     <script src="../cod/js/js.js" type="text/javascript" charset="utf-8"></script> 	<!--SWEETALERT JAVASCRIPT ALERTS WITH HTML--> 	<script src="../cod/sweetalert2-master/dist/sweetalert2.min.js"></script> 	<link rel="stylesheet" type="text/css" href="../cod/sweetalert2-master/dist/sweetalert2.css"> 	<!--END JQUERY PARSE AND SWEET ALERT--></head><body>			<% include partitions/pmenu.ejs %>		<% include partitions/navmenu.ejs %>		</br>				<table align="center" class="NoHover">					<tr>						<td><a href="/mainclients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Active Clients</b></font></a></td>						<td><a href="/clients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Pending</b></font></a></td>							<td><a href="/reservations" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Reservations</b></font></a></td>					</tr>				</table>			</br>		<div id="mainDiv">		<fieldset id="mainField">		</br>	<span class="font-3">Reservations-<%= user.get("Name")%></span></br>		<table align="right" class="NoHover">					<tr>				<td><a href="/<%=user.id%>" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Client's Information</b></font></a></td>						<td><a href="#" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>WareHouse</b></font></a></td>						<td><a href="#" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Reservations</b></font></a></td>					</tr>				</table>		</br>	</br>	</br>	<table class="pure-table pure-table-bordered">	    <thead>	        <tr>				<th><font style="color: white;">Status</font></th>				<th><font style="color: white;">Client</font></th>	            <th><font style="color: white;">Arrival</font></th>	            <th><font style="color: white;">Departure</font></th>	            <th><font style="color: white;">Airline</font></th>				<th><font style="color: white;">Flight Number</font></th>				<th><font style="color: white;">Contact Phone</font></th>				<th><font style="color: white;">Destination Address</font></th>			    </tr>	    </thead>	    <tbody>	<% trips.forEach(function(trip){ %>	        <tr id="onhoverTable">				<td><div style='text-align:center;'>			<img src='<%= trip.get("Icon") %>' width='25' height='25' alt='Status' style="cursor:pointer;" id='<%= trip.get("Status") %>' onclick="<%= trip.get("Status")%>C('<%= trip.id %>', this.id)"></div><%= trip.get("Status") %></td>			    <td><a href="/<%= trip.get('ClientID')%>" target="blank" style="text-decoration: none;"><%= trip.get("ClientName") %></td>				<td><%= trip.get("Arrival") %></td>				<td><%= trip.get("Departure") %></td>				<td><%= trip.get("Airline") %></td>				<td><%= trip.get("Flight") %></td>				<td><%= trip.get("Telephone") %></td>				<td><%= trip.get("Destination") %></td>				<td><a href="trip/<%=trip.id%>" target="blank"><img src='/web/img/icons/view.png' width="25" height="25" alt="Status" style="cursor:pointer;"></a></td>								<td><img src='<%= trip.get("Status2")%>' id='<%= trip.get("Status") %>' width="25" height="25" alt="Change" onclick="<%= trip.get("Status")%>D('<%= trip.id %>', this.id)" style="cursor:pointer;"></td>	        </tr>	 <% }) %>	    </tbody>	</table></fieldset></div></body></html>