" href="/cl_reserv/<%=user.id%>"><p class="pfont" style="color: rgb(103,122,133);">Reservations</p></a></legend>					</br>	<span class="font-3">Reservations-<%= user.get("Name")%></span></br>	</br>	<table class="pure-table pure-table-bordered">	    <thead>	        <tr>				<th><font style="color: white;">Status</font></th>				<th><font style="color: white;">Client</font></th>	            <th><font style="color: white;">Arrival</font></th>	            <th><font style="color: white;">Departure</font></th>	            <th><font style="color: white;">Airline</font></th>				<th><font style="color: white;">Flight Number</font></th>				<th><font style="color: white;">Contact Phone</font></th>				<th><font style="color: white;">Destination Address</font></th>			    </tr>	    </thead>	    <tbody>	<% trips.forEach(function(trip){ %>	        <tr id="onhoverTable">				<td><div style='text-align:center;'>			<img src='<%= trip.get("Icon") %>' width='25' height='25' alt='Status' style="cursor:pointer;" id='<%= trip.get("Status") %>' onclick="<%= trip.get("Status")%>C('<%= trip.id %>', this.id)"></div><%= trip.get("Status") %></td>			    <td><a href="/<%= trip.get('ClientID')%>" target="blank" style="text-decoration: none;"><p class="pfont"><%= trip.get("ClientName") %></p></td>				<td><p class="pfont"><%= trip.get("Arrival") %></p></td>				<td><p class="pfont"><%= trip.get("Departure") %></p></td>				<td><p class="pfont"><%= trip.get("Airline") %></p></td>				<td><p class="pfont"><%= trip.get("Flight") %></p></td>				<td><p class="pfont"><%= trip.get("Telephone") %></p></td>				<td><p class="pfont"><%= trip.get("Destination") %></p></td>				<td><a href="trip/<%=trip.id%>"><img src='/web/img/icons/view.png' width="25" height="25" alt="Status" style="cursor:pointer;"></a></td>								<td><img src='<%= trip.get("Status2")%>' id='<%= trip.get("Status") %>' width="25" height="25" alt="Change" onclick="<%= trip.get("Status")%>D('<%= trip.id %>', this.id)" style="cursor:pointer;"></td>	        </tr>	 <% }) %>	    </tbody>	</table></fieldset></div></body></html>