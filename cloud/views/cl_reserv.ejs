" href="/cl_reserv/<%=user.id%>"><p class="pfont" style="color: rgb(103,122,133);">Reservations</p></a></legend>					</br>	<span class="font-3">Reservations-<%= user.get("Name")%></span></br>        <form class="pure-form" method="post" action="/cl_warehouse" align="right">			    <fieldset> 			               <label for="filterby" class="pfont">By:</label>			               <select  id="filterby" name="filterby">							    <!--IF ALL SHOW DATE SELECT OPTIONS-->							   <option value="All">All</option>							    <!--IF CONSIGNEE SHOW DATE SELECT OPTIONS AND INPUT TO SPECIFY CONSIGNEE -->			                   <option value="Consignee">Consignee</option>							    <!--IF SHIPPER SHOW DATE SELECT OPTIONS AND INPUT TO SPECIFY SHIPPER-->			                   <option value="Shipper">Shipper</option>							    <!--IF RECEIPT # SHOW DATE SELECT OPTIONS AND INPUT TO SPECIFY RECEIPT-->			                   <option value="Receipt">Receipt #</option>							   <!--IF BY DATE ONLY SHOW DATE SELECT OPTIONS-->			                   <option value="Date">Date</option>							      <!--IF TRANSPORT SHOW DATE SELECT OPTIONS AND INPUT TO SPECIFY TRANSPORT-->			                   <option value="Transport">Transport</option>			               </select>						   <!-- IF ALL OR DATE SELECTED THIS SEARCH INPUT IS NOT USE-->						   <input type="text" placeholder="Search"  id="searchtext" name="searchtext">				   						   <label class="pfont">Date:</label>						   <select id="filterdate" name="filterdate">							   <option value="Today">Today</option>							   <option value="Yestarday">Yesterday</option>							   <option value="This week">This week</option>							   <option value="This month">This month</option>							   <option value="Last month">Last month</option>								   <option value="Current year">Current Year</option>							   <option value="Specific">Date Range</option>						   </select>	                                                      <input type='hidden' id='usr' name='usr' value="<%=user.id%>">                           		   			        <button type="submit" class="pure-button pure-button-primary"><p class="pfont">Search</p></button>			    </fieldset>			</form>	</br>	<table class="pure-table pure-table-bordered">	    <thead>	        <tr>				<th><font style="color: white;">Status</font></th>				<th><font style="color: white;">Client</font></th>	            <th><font style="color: white;">Arrival</font></th>	            <th><font style="color: white;">Departure</font></th>	            <th><font style="color: white;">Airline</font></th>				<th><font style="color: white;">Flight Number</font></th>				<th><font style="color: white;">Contact Phone</font></th>				<th><font style="color: white;">Destination Address</font></th>			    </tr>	    </thead>	    <tbody>	<% trips.forEach(function(trip){ %>	        <tr id="onhoverTable">				<td><div style='text-align:center;'>			<img src='<%= trip.get("Icon") %>' width='25' height='25' alt='Status' style="cursor:pointer;" id='<%= trip.get("Status") %>' onclick="<%= trip.get("Status")%>C('<%= trip.id %>', this.id)"></div><%= trip.get("Status") %></td>			    <td><a href="/<%= trip.get('ClientID')%>" target="blank" style="text-decoration: none;"><p class="pfont"><%= trip.get("ClientName") %></p></td>				<td><p class="pfont"><%= trip.get("Arrival") %></p></td>				<td><p class="pfont"><%= trip.get("Departure") %></p></td>				<td><p class="pfont"><%= trip.get("Airline") %></p></td>				<td><p class="pfont"><%= trip.get("Flight") %></p></td>				<td><p class="pfont"><%= trip.get("Telephone") %></p></td>				<td><p class="pfont"><%= trip.get("Destination") %></p></td>				<td><a href="trip/<%=trip.id%>"><img src='/web/img/icons/view.png' width="25" height="25" alt="Status" style="cursor:pointer;"></a></td>								<td><img src='<%= trip.get("Status2")%>' id='<%= trip.get("Status") %>' width="25" height="25" alt="Change" onclick="<%= trip.get("Status")%>D('<%= trip.id %>', this.id)" style="cursor:pointer;"></td>	        </tr>	 <% }) %>	    </tbody>	</table></fieldset></div> <script>	$("#filterby").change(function(){			if($("#filterby").val() == "Date")		{			$("#searchtext").replaceWith("<input type='text'  placeholder='From' id='searchtext' name='searchtext'>");                        $("#filterdate").replaceWith("<input type='text'  placeholder='To' id='filterdate' name='filterdate'>");            			$( "#searchtext" ).datepicker();            $( "#filterdate" ).datepicker();		}        else        {			$("#searchtext").replaceWith("<input type='text' placeholder='Search' id='searchtext' name='searchtext'>");            $("#filterdate").replaceWith("<select id='filterdate' name='filterdate'><option value='Today'>Today</option> <option value='Yestarday'>Yesterday</option><option value='This week'>This week</option><option value='This month'>This month</option><option value='Last month'>Last month</option><option value='Current year'>Current Year</option><option value='Specific'>Date Range</option></select>");                    }	});	</script></body></html>