/br>		<table class="pure-table pure-table-bordered" align="left" style="margin-right: 90px; z-index:10;">		    <thead>		        <tr>		            <th><font style="color: white;"><p class="pfont">Name</p></font></th>		            <th><font style="color: white;"><p class="pfont">Username</p></font></th>					 <th><font style="color: white;"><p class="pfont">Email</p></font></th>				    </tr>		    </thead>		    <tbody>						<% staffs.forEach(function(staff) { %>					        <tr id="onhoverTable">		       <td><a href='/staff/<%=staff.id%>' style="text-decoration: none;" target="blank"><p class="pfont"><%= staff.get("Name") %> <%= staff.get("LastName") %></p></a></td>					<td><p class="pfont"><%= staff.get("username") %></p></td>					<td><p class="pfont"><%= staff.get("email") %></p></td>					<td><a href="/staff/<%=staff.id%>"><img src="web/img/icons/view.png" width="24" height="24" alt="View"></td>				<td><a href="/deleteStaff:<%=staff.id%>"><img src="web/img/icons/delete.png" width="24" height="24" alt="View"></a></td>		        </tr>		 <% }) %> 		    </tbody>		</table>		<div class="section" id="addStaffDiv">	<div class="cont">	<form name="createStaff" action="/createStaff" method="post" onsubmit="return valRegStaff()">	<div class="cont-2">					<div class="cont-3"><p class="para"><span class="font">New Staff Member:</span></p></div>				</div>				<div class="cont-4">					<div class="cont-5"><p class="para-2"><span class="font-2">Name:</span></p></div>				</div>				<div class="cont-6">					<input type="text" name="staffName" class="input">				</div>				<div class="cont-7">					<div class="cont-8"><p class="para-3"><span class="font-2">Last Name</span><span class="font-4">:</span></p></div>				</div>				<div class="cont-9">					<input type="text" name="staffLastName" class="input-2">				</div>				<div class="cont-10">					<div class="cont-11"><p class="para-4"><span class="font-5">Email:</span></p></div>				</div>				<div class="cont-12">					<input type="email" name="staffEmail" class="input-3">				</div>				<div class="cont-13">					<div class="cont-14"><p class="para-5"><span class="font-6">Username:</span></p></div>				</div>				<div class="cont-15">					<input type="text" name="staffUsrname" class="input-4">				</div>				<div class="cont-16">					<div class="cont-17"><p class="para-6"><span class="font-7">Password:</span></p></div>				</div>				<div class="cont-18">					<input type="password" name="staffPassword" class="input-5">				</div>				<div class="cont-19">					<div class="cont-20"><p class="para-7"><span class="font-8">Confirm Pasword:</span></p></div>				</div>				<div class="cont-21">					<input type="password" name="staffCpass" class="input-6">				</div>				<div class="cont-22">					<input type="submit" class="button" value="Create">				</div>			</form>			</div>		</div></br></fieldset></div></body></html>