<!doctype html><html lang="en"><head>	       <meta charset="utf-8">	   <meta name="viewport" content="width=device-width, initial-scale=1.0">	   <meta name="description" content="Directory">	   <title>Clients Reservations</title>	   <link rel="stylesheet" href="cod/css/pure-min.css">	   <link rel="stylesheet" href="cod/css/grids-responsive-min.css">	       <link rel="stylesheet" href="cod/css/style.css">	   	<!--JQUERY, PARSE AND SWEET ALERT, JAVASCRIPT-->	       <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>	       <script src="https://www.parsecdn.com/js/parse-1.2.8.min.js"></script>		       <link rel="stylesheet" href="cod/jquery/jquery-ui.css" type="text/css" media="screen" title="no title" charset="utf-8">	       <script src="cod/js/js.js" type="text/javascript" charset="utf-8"></script>	   	<!--SWEETALERT JAVASCRIPT ALERTS WITH HTML-->	   	<script src="cod/sweetalert2-master/dist/sweetalert2.min.js"></script>	   	<link rel="stylesheet" type="text/css" href="cod/sweetalert2-master/dist/sweetalert2.css">	   	<!--END JQUERY PARSE AND SWEET ALERT--></head><body>		<% include partitions/pmenu.ejs %>	</br>	</br>	<% include partitions/navmenu.ejs %>		<table align="center" class="NoHover">		<tr>			<td><a href="/mainclients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Active Clients</b></font></a></td>			<td><a href="/clients" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Pending</b></font></a></td>			<td><a href="#" class="dc_gradient_button rounded"><font style="color: #606A76;"><b>Reservations</b></font></a></td>		</tr>	</table>	</br>	<div style="position:relative; left:15%;">	<form class="pure-form">	    <fieldset>	        <legend>Find a trip:</legend>	               <label for="state">By:</label>	               <select id="state" >					   <option>Status</option>	                   <option>Client's name</option>					   <option>Arrival's date</option>					   <option>Departure's date</option>	                   <option>Airline</option>	                   <option>Flight Number</option>					    <option>People</option>	                   <option>Destination</option>	                   <option>Email</option>	               </select>			     <input type="text" placeholder="Search">	               <label for="state">Show First:</label>	               <input type="number" value="5" style="width:60px;">			  	        <button type="submit" class="pure-button pure-button-primary">Search</button>	    </fieldset>	</form>	</div></br>	<table class="pure-table pure-table-bordered" style="position:absolute; top:233px; left:17%; max-width: 70%; 	min-width: 70%; z-index:8;">	    <thead>	        <tr>				<th><font style="color: white;">Status</font></th>				<th><font style="color: white;">Client</font></th>	            <th><font style="color: white;">Arrival</font></th>	            <th><font style="color: white;">Departure</font></th>	            <th><font style="color: white;">Airline</font></th>				<th><font style="color: white;">Flight Number</font></th>				<th><font style="color: white;">Contact Phone</font></th>				<th><font style="color: white;">Destination Address</font></th>			    </tr>	    </thead>	    <tbody>	<% reservations.forEach(function(reservation){ %>	        <tr id="onhoverTable">				<td><div style='text-align:center;'>			<img src='<%= reservation.get("Icon") %>' width='25' height='25' alt='Status' style="cursor:pointer;" id='<%= reservation.get("Status") %>' onclick="<%= reservation.get("Status")%>C('<%= reservation.id %>', this.id)"></div><%= reservation.get("Status") %></td>			    <td><a href="/<%= reservation.get('ClientID')%>" target="blank" style="text-decoration: none;"><%= reservation.get("ClientName") %></td>				<td><%= reservation.get("Arrival") %></td>				<td><%= reservation.get("Departure") %></td>				<td><%= reservation.get("Airline") %></td>				<td><%= reservation.get("Flight") %></td>				<td><%= reservation.get("Telephone") %></td>				<td><%= reservation.get("Destination") %></td>				<td><a href="trip/<%=reservation.id%>" target="blank"><img src='/web/img/icons/view.png' width="25" height="25" alt="Status" style="cursor:pointer;"></a></td>								<td><img src='<%= reservation.get("Status2")%>' id='<%= reservation.get("Status") %>' width="25" height="25" alt="Change" onclick="<%= reservation.get("Status")%>D('<%= reservation.id %>', this.id)" style="cursor:pointer;"></td>	        </tr>	 <% }) %>	    </tbody>	</table>	</body></html>